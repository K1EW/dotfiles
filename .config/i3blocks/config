separator=true
separator_block_width=25

[time]
command=echo " $(date '+%Y %b %d  %H:%M:%S')"
color=#ebdbb2
interval=5

[keyboard-layout]
command=echo "⌨ $(xkblayout-state print '%s')"
color=#ebdbb2
interval=5

[speaker]
command=$HOME/.config/i3blocks/speaker.py
interval=60
signal=1

[network]
command=$HOME/.config/i3blocks/network.py
interval=5

[battery]
command=$HOME/.config/i3blocks/battery.py
interval=5
